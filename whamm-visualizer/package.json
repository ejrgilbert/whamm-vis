{
  "name": "whamm-visualizer",
  "displayName": "Whamm Visualizer",
  "description": "Visualizer for Whamm Modules",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "whamm-visualizer.open-text-display",
        "category": "Whamm Visualizer",
        "title": "Open Text Display"
      },
      {
        "command": "whamm-visualizer.open-pie-display",
        "category": "Whamm Visualizer",
        "title": "Open Pie Display"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/papaparse": "^5.3.16",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "ansi-to-html": "^0.7.2",
    "echarts": "^5.6.0",
    "papaparse": "^5.5.3",
    "@vscode/webview-ui-toolkit": "1.2.2"
  }
}
